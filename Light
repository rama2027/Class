node('master')
{
stage('clone')
{
git 'https://github.com/rama2027/Class.git'
}
stage('Credtials')
{
sh 'sh $WORKSPACE/assumerole.sh'
withEnv(["AWS_SECRET_ACCESS_KEY=$AWS_SECRET_ACCESS_KEY"])
withEnv(["AWS_SESSION_TOKEN=$AWS_SESSION_TOKEN"])
withEnv(["AWS_ACCESS_KEY_ID=$AWS_ACCESS_KEY_ID"]){
sh '$AWS_ACCESS_KEY_ID'
  }
}
stage('Describe')
{
sh 'sh $WORKSPACE/describe.sh'
}
stage('clean')
{
cleanWs()
}
}
