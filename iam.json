{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "IAM Role CFT",
    "Resources": {
      "Role": {
         "Type": "AWS::IAM::Role",
         "Properties": {
            "AssumeRolePolicyDocument": {
              "Version": "2012-10-17",
                    "Statement": [
                        {
                            "Effect": "Allow",
                            "Principal": {
                                "Service": [
                                    "ec2.amazonaws.com"
                                ]
                            },
                            "Action": [
                                "sts:AssumeRole"
                            ]
                        }
                    ]
                },
            "Path": "/",
            "RoleName": "Cloudformationtest",
            "Policies": [
              {
                        "PolicyName": "Cloudformationtest",
                        "PolicyDocument": {
                            "Version": "2012-10-17",
                            "Statement": [
                                {
                                    "Effect": "Allow",
                                    "Action": "*",
                                    "Resource": "*"
                                }
                            ]
                        }
                    }
            ]
         }
      }

    },
    "Outputs": {
      "test": {
        "Description": "test",
        "Value": { "Ref": "Role" }
      }
    }
}
